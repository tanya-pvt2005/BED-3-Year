{
  "name": "wb",
  "version": "0.0.1",
  "description": "Another weibo API Node.js client.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --check-leaks -R spec -t 5000 -r should-http test/*.test.js",
    "test-cov": "node --harmony node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --check-leaks -t 5000 -r should-http test/*.test.js",
    "test-travis": "node --harmony node_modules/.bin/istanbul cover node_modules/.bin/_mocha --report lcovonly -- --check-leaks -t 5000 -r should-http test/*.test.js",
    "jshint": "jshint .",
    "autod": "autod -w --prefix '~' && npm run cnpm",
    "cnpm": "npm install --registry=https://registry.npm.taobao.org",
    "contributors": "contributors -f plain -o AUTHORS"
  },
  "dependencies": {

  },
  "devDependencies": {
    "autod": "*",
    "contributors": "*",
    "should": "*",
    "should-http": "*",
    "jshint": "*",
    "istanbul-harmony": "*",
    "mocha": "*"
  },
  "homepage": "https://github.com/node-modules/wb",
  "repository": {
    "type": "git",
    "url": "git://github.com/node-modules/wb.git",
    "web": "https://github.com/node-modules/wb"
  },
  "bugs": {
    "url": "https://github.com/node-modules/wb/issues",
    "email": "fengmk2@gmail.com"
  },
  "keywords": [
    "wb"
  ],
